{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/App/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true
    },
    "func": {
      "path": "src/App/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates": {
        "component": "GRC_Template/genericFunction/TemplateName.jsx",
        "test": "GRC_Template/Readme.md"
      }
    },
    "layout": {
      "path": "src/App/components/layouts",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates": {
        "component": "GRC_Template/genericLayout/TemplateName.jsx",
        "test": "GRC_Template/Readme.md"
      }
    }
  }
}